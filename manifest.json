{
    "manifest_version": 2,
    "name": "My Cool Extension",
    "version": "0.1",
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": [
              "jquery.js",
              "content.js"
            ]
        }
      ],
      "browser_action": {
        "default_icon": "a.png",
        "default_popup": "popup.html"
      }
  }